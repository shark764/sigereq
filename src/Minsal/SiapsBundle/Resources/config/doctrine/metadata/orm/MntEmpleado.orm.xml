<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MntEmpleado" table="mnt_empleado">
    <indexes>
      <index name="IDX_2138DDC94F664059" columns="id_cargo_empleado"/>
      <index name="IDX_2138DDC97DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_2138DDC9DA799B26" columns="id_establecimiento_externo"/>
      <index name="IDX_2138DDC9B13434FE" columns="id_tipo_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_empleado_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Nombre del empleado</option>
      </options>
    </field>
    <field name="apellido" type="string" column="apellido" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Apellidos del Empleado</option>
      </options>
    </field>
    <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="true">
      <options>
        <option name="comment">Fecha de nacimiento del empleado</option>
      </options>
    </field>
    <field name="dui" type="string" column="dui" length="12" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">DUI del empleado</option>
      </options>
    </field>
    <field name="numeroJuntaVigilancia" type="string" column="numero_junta_vigilancia" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Numero de junta de vigilancia en caso de que sea medico</option>
      </options>
    </field>
    <field name="numeroCelular" type="string" column="numero_celular" length="10" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Telefono celular de contacto</option>
      </options>
    </field>
    <field name="correoElectronico" type="string" column="correo_electronico" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Correo electronico</option>
      </options>
    </field>
    <field name="correlativo" type="smallint" column="correlativo" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">UTILIZADO PARA LOS QUE POSEE VERSIÃ“N ANTIGUA DE CODIGO SIAP</option>
      </options>
    </field>
    <field name="firmaDigital" type="text" column="firma_digital" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Campo para setearle el hash de la firma digital del empleado</option>
      </options>
    </field>
    <field name="idUserReg" type="smallint" column="id_user_reg" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Almacena el id del usuario que ingresa el registro</option>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true">
      <options>
        <option name="comment">almacena la fecha y hora de ingreso del registro</option>
      </options>
    </field>
    <field name="idUserMod" type="smallint" column="id_user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Almacena el id del usuario que modifico el registro</option>
      </options>
    </field>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true">
      <options>
        <option name="comment">almacena la fecha y hora de modificacion del registro</option>
      </options>
    </field>
    <field name="nombreEmpleado" type="string" column="nombre_empleado" length="200" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Nombre Unido para los modulos de PHP Puro y POstgresql</option>
      </options>
    </field>
    <field name="habilitado" type="boolean" column="habilitado" nullable="true">
      <options>
        <option name="comment">Almacena si el registro esta habilitado</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="residente" type="boolean" column="residente" nullable="true">
      <options>
        <option name="default"/>
      </options>
    </field>
    <field name="idNuevoEmpleado" type="integer" column="id_nuevo_empleado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idTipoEmpleado" target-entity="MntTipoEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimientoExterno" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_establecimiento_externo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCargoEmpleado" target-entity="MntCargoempleados" fetch="LAZY">
      <join-columns>
        <join-column name="id_cargo_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
