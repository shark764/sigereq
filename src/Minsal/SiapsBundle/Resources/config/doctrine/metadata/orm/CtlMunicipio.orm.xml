<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlMunicipio" table="ctl_municipio">
    <indexes>
      <index name="IDX_914172ED6325E299" columns="id_departamento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_municipio_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Nombre del municipio</option>
      </options>
    </field>
    <field name="codigoCnr" type="string" column="codigo_cnr" length="5" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Codigo asignado por la Digestyc para un municipio en especifico</option>
      </options>
    </field>
    <field name="abreviatura" type="string" column="abreviatura" length="5" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Abreviatura asignada al municipio</option>
      </options>
    </field>
    <many-to-one field="idDepartamento" target-entity="CtlDepartamento" fetch="LAZY">
      <join-columns>
        <join-column name="id_departamento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
