<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlCreacionExpediente" table="ctl_creacion_expediente">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_creacion_expediente_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="area" type="string" column="area" length="25" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Nombre del área de creación del expediente</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
