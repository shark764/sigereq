      </options>
    </field>
    <field name="numeroDocIdeProporDatos" type="string" column="numero_doc_ide_propor_datos" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Número del documento de identidad seleccionado para el proporciono datos</option>
      </options>
    </field>
    <field name="observacion" type="text" column="observacion" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Observaciones del expediente del paciente, Fusiones de expedientes, etc</option>
      </options>
    </field>
    <field name="conocidoPor" type="string" column="conocido_por" length="70" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Sobre-Nombre con el que es conocido popularmente el paciente</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">Estado del paciente. 1=Vivo; 2= Fallecido; 3=Inactivo </option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="idPacienteInicial" type="bigint" column="id_paciente_inicial" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Número global del paciente</option>
      </options>
    </field>
    <field name="fechaRegistro" type="datetime" column="fecha_registro" nullable="true">
      <options>
        <option name="comment">Fecha en que fue creado el registro del paciente</option>
      </options>
    </field>
    <field name="fechaMod" type="datetime" column="fecha_mod" nullable="true">
      <options>
        <option name="comment">Fecha de última modificación de datos de paciente
</option>
      </options>
    </field>
    <field name="cotizante" type="boolean" column="cotizante" nullable="true"/>
