    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_paciente_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="primerNombre" type="string" column="primer_nombre" length="25" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Primer nombre del paciente</option>
      </options>
    </field>
    <field name="segundoNombre" type="string" column="segundo_nombre" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Segundo nombre del paciente es opcional</option>
      </options>
    </field>
    <field name="tercerNombre" type="string" column="tercer_nombre" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Tercer nombre del paciente es opcional</option>
      </options>
    </field>
    <field name="primerApellido" type="string" column="primer_apellido" length="25" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Primer apellido del paciente</option>
      </options>
    </field>
    <field name="segundoApellido" type="string" column="segundo_apellido" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Segundo apellido del paciente es opcional</option>
      </options>
    </field>
    <field name="apellidoCasada" type="string" column="apellido_casada" length="25" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Apellido de casada para paciente mujer</option>
      </options>
    </field>
    <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="true">
      <options>
        <option name="comment">Fecha de nacimiento del paciente</option>
      </options>
    </field>
    <field name="horaNacimiento" type="time" column="hora_nacimiento" nullable="true">
      <options>
        <option name="comment">Hora de nacimiento en caso se conociera</option>
      </options>
    </field>
    <field name="numeroDocIdePaciente" type="string" column="numero_doc_ide_paciente" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Número del documento de identidad seleccionado para el paciente</option>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="200" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Direccion de donde vive el paciente</option>
      </options>
    </field>
    <field name="telefonoCasa" type="string" column="telefono_casa" length="10" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Teléfono de contacto de casa en caso existiera</option>
      </options>
    </field>
    <field name="lugarTrabajo" type="string" column="lugar_trabajo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Lugar donde trabaja el paciente, En caso trabajara</option>
      </options>
    </field>
    <field name="telefonoTrabajo" type="string" column="telefono_trabajo" length="10" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Teléfono del trabajo, en caso existiera</option>
      </options>
    </field>
    <field name="nombrePadre" type="string" column="nombre_padre" length="80" nullable="true">
