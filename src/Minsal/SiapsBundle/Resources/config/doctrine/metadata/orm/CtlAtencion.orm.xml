<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlAtencion" table="ctl_atencion">
    <indexes>
      <index name="IDX_5075A312C5DC305D" columns="id_atencion_padre"/>
      <index name="IDX_5075A3125168C468" columns="id_tipo_atencion"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_atencion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Nombre de la atención a recibir por el paciente</option>
      </options>
    </field>
    <field name="codigoBusqueda" type="string" column="codigo_busqueda" length="6" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Codigo de busqueda de la atención en un paciente. Que se utiliza en Laboratorio y Citas</option>
      </options>
    </field>
    <many-to-one field="idTipoAtencion" target-entity="CtlTipoAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAtencionPadre" target-entity="CtlAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_atencion_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
