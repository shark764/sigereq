<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlTipoEstablecimiento" table="ctl_tipo_establecimiento">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_tipo_establecimiento_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idInstitucion" type="integer" column="id_institucion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
