<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MntExpediente" table="mnt_expediente">
    <indexes>
      <index name="IDX_E4950F572DF9F9B6" columns="id_banco_de_leche"/>
      <index name="IDX_E4950F577DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_E4950F57961045CB" columns="id_paciente"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_numero_expediente" columns="numero"/>
      <unique-constraint name="idx_id_id_paciente" columns="id,id_paciente"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_expediente_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="numero" type="string" column="numero" length="12" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="habilitado" type="boolean" column="habilitado" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="true"/>
    <field name="horaCreacion" type="time" column="hora_creacion" nullable="true"/>
    <field name="numeroTemporal" type="boolean" column="numero_temporal" nullable="true">
      <options>
        <option name="default"/>
      </options>
    </field>
    <field name="expedienteFisicoEliminado" type="boolean" column="expediente_fisico_eliminado" nullable="true">
      <options>
        <option name="default"/>
      </options>
    </field>
    <field name="cun" type="boolean" column="cun" nullable="true">
      <options>
        <option name="default"/>
      </options>
    </field>
    <field name="idRegistroSiap" type="bigint" column="id_registro_siap" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idPaciente" target-entity="MntPaciente" fetch="LAZY">
      <join-columns>
        <join-column name="id_paciente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBancoDeLeche" target-entity="BlhBancoDeLeche" fetch="LAZY">
      <join-columns>
        <join-column name="id_banco_de_leche" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
