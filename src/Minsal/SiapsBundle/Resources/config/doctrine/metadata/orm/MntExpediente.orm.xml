<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MntExpediente" table="mnt_expediente">
    <indexes>
      <index name="IDX_E4950F577DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_E4950F57961045CB" columns="id_paciente"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_expediente_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="numero" type="string" column="numero" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idPaciente" target-entity="MntPaciente" fetch="LAZY">
      <join-columns>
        <join-column name="id_paciente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
