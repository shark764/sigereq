<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhSeguimientoReceptor" table="blh_seguimiento_receptor">
    <indexes>
      <index name="IDX_EB08C6D4B91944F2" columns="id_receptor"/>
      <index name="IDX_EB08C6D4D8A5832B" columns="id_user_reg"/>
      <index name="IDX_EB08C6D42DF9F9B6" columns="id_banco_de_leche"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_seguimiento_receptor_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="tallaReceptor" type="decimal" column="talla_receptor" precision="7" scale="4" nullable="false"/>
    <field name="pesoSeguimiento" type="decimal" column="peso_seguimiento" precision="8" scale="4" nullable="false"/>
    <field name="pcSeguimiento" type="decimal" column="pc_seguimiento" precision="6" scale="4" nullable="false"/>
    <field name="gananciaDiaPeso" type="decimal" column="ganancia_dia_peso" precision="7" scale="4" nullable="false"/>
    <field name="semana" type="integer" column="semana" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaSeguimiento" type="date" column="fecha_seguimiento" nullable="false"/>
    <field name="gananciaDiaTalla" type="decimal" column="ganancia_dia_talla" precision="7" scale="4" nullable="false"/>
    <field name="complicaciones" type="string" column="complicaciones" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="observacion" type="string" column="observacion" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="periodoEvaluacion" type="integer" column="periodo_evaluacion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="gananciaDiaPc" type="decimal" column="ganancia_dia_pc" precision="7" scale="4" nullable="true"/>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idBancoDeLeche" target-entity="BlhBancoDeLeche" fetch="LAZY">
      <join-columns>
        <join-column name="id_banco_de_leche" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idReceptor" target-entity="BlhReceptor" fetch="LAZY">
      <join-columns>
        <join-column name="id_receptor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
