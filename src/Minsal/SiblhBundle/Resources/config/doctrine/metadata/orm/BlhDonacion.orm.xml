<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhDonacion" table="blh_donacion">
    <indexes>
      <index name="fk_banco_de_leche_donacion" columns="id_banco_de_leche"/>
      <index name="IDX_13A0CBCB8653A7AF" columns="id_centro_recoleccion"/>
      <index name="IDX_13A0CBCB54F03532" columns="id_donante"/>
      <index name="IDX_13A0CBCBD8A5832B" columns="id_user_reg"/>
      <index name="IDX_13A0CBCBD4737338" columns="id_responsable_donacion"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_donacion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="codigoDonante" type="string" column="codigo_donante" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaDonacion" type="date" column="fecha_donacion" nullable="false"/>
    <field name="responsableDonacion" type="string" column="responsable_donacion" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idResponsableDonacion" target-entity="MntEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_responsable_donacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBancoDeLeche" target-entity="BlhBancoDeLeche" fetch="LAZY">
      <join-columns>
        <join-column name="id_banco_de_leche" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDonante" target-entity="BlhDonante" fetch="LAZY">
      <join-columns>
        <join-column name="id_donante" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCentroRecoleccion" target-entity="BlhCtlCentroRecoleccion" fetch="LAZY">
      <join-columns>
        <join-column name="id_centro_recoleccion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
