<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhExamenDonante" table="blh_examen_donante">
    <indexes>
      <index name="fk__donante_examen_donante" columns="id_examen"/>
      <index name="IDX_BECE40754F03532" columns="id_donante"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_examen_donante_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="resultadoExamen" type="string" column="resultado_examen" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idExamen" target-entity="BlhExamen" fetch="LAZY">
      <join-columns>
        <join-column name="id_examen" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDonante" target-entity="BlhDonante" fetch="LAZY">
      <join-columns>
        <join-column name="id_donante" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
