<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhTemperaturaEnfriamiento" table="blh_temperatura_enfriamiento">
    <indexes>
      <index name="IDX_3985D55C9D483370" columns="id_pasteurizacion"/>
      <index name="IDX_3985D55CD8A5832B" columns="id_user_reg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_temperatura_enfriamiento_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="temperaturaE" type="integer" column="temperatura_e" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="horaInicioE" type="time" column="hora_inicio_e" nullable="true"/>
    <field name="horaFinalE" type="time" column="hora_final_e" nullable="true"/>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPasteurizacion" target-entity="BlhPasteurizacion" fetch="LAZY">
      <join-columns>
        <join-column name="id_pasteurizacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
