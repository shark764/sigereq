<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhAcidez" table="blh_acidez">
    <indexes>
      <index name="fk_frasco_reco_acid" columns="id_frasco_recolectado"/>
      <index name="IDX_E6CCE10CD8A5832B" columns="id_user_reg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_acidez_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="acidez1" type="integer" column="acidez1" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="acidez2" type="integer" column="acidez2" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="acidez3" type="integer" column="acidez3" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="factor" type="decimal" column="factor" precision="6" scale="4" nullable="false"/>
    <field name="resultado" type="decimal" column="resultado" precision="6" scale="4" nullable="false"/>
    <field name="mediaAcidez" type="decimal" column="media_acidez" precision="6" scale="4" nullable="false"/>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFrascoRecolectado" target-entity="BlhFrascoRecolectado" fetch="LAZY">
      <join-columns>
        <join-column name="id_frasco_recolectado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
