<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhRolMenu" table="blh_rol_menu">
    <indexes>
      <index name="fk_rol_rol_menu" columns="id_rol"/>
      <index name="fk_menu_rol_menu" columns="id_menu"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_rol_menu_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idMenu" target-entity="BlhMenu" fetch="LAZY">
      <join-columns>
        <join-column name="id_menu" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idRol" target-entity="BlhRol" fetch="LAZY">
      <join-columns>
        <join-column name="id_rol" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
