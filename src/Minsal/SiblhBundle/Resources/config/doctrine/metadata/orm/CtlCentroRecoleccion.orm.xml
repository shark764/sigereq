<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlCentroRecoleccion" table="ctl_centro_recoleccion">
    <indexes>
      <index name="IDX_6A39858DD8A5832B" columns="id_user_reg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_centro_recoleccion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
