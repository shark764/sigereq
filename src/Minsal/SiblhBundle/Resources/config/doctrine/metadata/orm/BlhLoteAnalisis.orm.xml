<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhLoteAnalisis" table="blh_lote_analisis">
    <indexes>
      <index name="IDX_232421CCD8A5832B" columns="id_user_reg"/>
      <index name="IDX_232421CC78720D11" columns="id_responsable_analisis"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_lote_analisis_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="codigoLoteAnalisis" type="string" column="codigo_lote_analisis" length="11" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaAnalisisFisicoQuimico" type="date" column="fecha_analisis_fisico_quimico" nullable="false"/>
    <field name="responsableAnalisis" type="string" column="responsable_analisis" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idResponsableAnalisis" target-entity="MntEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_responsable_analisis" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
