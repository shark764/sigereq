<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhBancoDeLeche" table="blh_banco_de_leche">
    <indexes>
      <index name="fk_establecimiento_banco_de_lec" columns="id_establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_banco_de_leche_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="codigoBancoDeLeche" type="string" column="codigo_banco_de_leche" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoBanco" type="string" column="estado_banco" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
