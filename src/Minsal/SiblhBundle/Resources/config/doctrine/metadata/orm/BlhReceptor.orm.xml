<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlhReceptor" table="blh_receptor">
    <indexes>
      <index name="fk_banco_de_leche_receptor" columns="id_banco_de_leche"/>
      <index name="fk_paciente_receptor" columns="id_paciente"/>
      <index name="IDX_6498D2A73181F7D" columns="id_curva_lubchenco"/>
      <index name="IDX_6498D2AD436DB02" columns="id_madre_donante"/>
      <index name="IDX_6498D2A701624C4" columns="id_expediente"/>
      <index name="IDX_6498D2AD8A5832B" columns="id_user_reg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="blh_receptor_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="codigoReceptor" type="string" column="codigo_receptor" length="14" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaRegistroBlh" type="date" column="fecha_registro_blh" nullable="true"/>
    <field name="procedencia" type="string" column="procedencia" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoReceptor" type="string" column="estado_receptor" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="edadDias" type="integer" column="edad_dias" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pesoReceptor" type="decimal" column="peso_receptor" precision="8" scale="4" nullable="false"/>
    <field name="duracionCpap" type="integer" column="duracion_cpap" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="clasificacionLubchengo" type="string" column="clasificacion_lubchengo" length="3" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="diagnosticoIngreso" type="string" column="diagnostico_ingreso" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="duracionNpt" type="integer" column="duracion_npt" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="apgarPrimerMinuto" type="decimal" column="apgar_primer_minuto" precision="6" scale="4" nullable="true"/>
    <field name="edadGestFur" type="decimal" column="edad_gest_fur" precision="6" scale="4" nullable="false"/>
    <field name="duracionVentilacion" type="integer" column="duracion_ventilacion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="edadGestBallard" type="decimal" column="edad_gest_ballard" precision="6" scale="4" nullable="false"/>
    <field name="pc" type="decimal" column="pc" precision="6" scale="4" nullable="false"/>
    <field name="tallaIngreso" type="decimal" column="talla_ingreso" precision="7" scale="4" nullable="true"/>
    <field name="apgarQuintoMinuto" type="decimal" column="apgar_quinto_minuto" precision="6" scale="4" nullable="true"/>
    <field name="usuario" type="integer" column="usuario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <many-to-one field="idPaciente" target-entity="MntPaciente" fetch="LAZY">
      <join-columns>
        <join-column name="id_paciente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idExpediente" target-entity="MntExpediente" fetch="LAZY">
      <join-columns>
        <join-column name="id_expediente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMadreDonante" target-entity="BlhDonante" fetch="LAZY">
      <join-columns>
        <join-column name="id_madre_donante" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCurvaLubchenco" target-entity="BlhCtlCurvaLubchenco" fetch="LAZY">
      <join-columns>
        <join-column name="id_curva_lubchenco" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBancoDeLeche" target-entity="BlhBancoDeLeche" fetch="LAZY">
      <join-columns>
        <join-column name="id_banco_de_leche" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
