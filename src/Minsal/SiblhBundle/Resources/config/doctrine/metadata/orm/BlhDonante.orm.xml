      <options>
        <option name="default"/>
      </options>
    </field>
    <many-to-one field="idExpediente" target-entity="MntExpediente" fetch="LAZY">
      <join-columns>
        <join-column name="id_expediente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idOcupacion" target-entity="CtlOcupacion" fetch="LAZY">
