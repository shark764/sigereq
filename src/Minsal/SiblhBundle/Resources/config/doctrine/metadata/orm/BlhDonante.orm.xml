    </field>
    <field name="ocupacion" type="string" column="ocupacion" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoCivil" type="string" column="estado_civil" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="escolaridad" type="string" column="escolaridad" length="15" nullable="true">
