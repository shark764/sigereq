      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registro" type="string" column="registro" length="12" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroDocumentoIdentificacion" type="string" column="numero_documento_identificacion" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="documentoIdentificacion" type="string" column="documento_identificacion" length="20" nullable="true">
