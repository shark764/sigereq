<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqAreaServicioAtencion" table="req_area_servicio_atencion">
    <indexes>
      <index name="IDX_FDA1E1E0477882F9" columns="id_jefe_departamento"/>
      <index name="IDX_FDA1E1E044A2C069" columns="id_area_atencion"/>
      <index name="IDX_FDA1E1E0DC0A4806" columns="id_servicio_atencion"/>
      <index name="IDX_FDA1E1E088863BBD" columns="id_servicio_externo"/>
      <index name="IDX_FDA1E1E0EF8B2BB4" columns="id_modalidad_atencion"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_modalidad_area_servicio_atencion" columns="id_area_atencion,id_servicio_atencion,id_servicio_externo,id_modalidad_atencion"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_area_servicio_atencion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <many-to-one field="idModalidadAtencion" target-entity="ReqCtlModalidadAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_modalidad_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idServicioExterno" target-entity="ReqCtlServicioExterno" fetch="LAZY">
      <join-columns>
        <join-column name="id_servicio_externo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idServicioAtencion" target-entity="ReqCtlServicioAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_servicio_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaAtencion" target-entity="ReqCtlAreaAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_area_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idJefeDepartamento" target-entity="ReqEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_jefe_departamento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
