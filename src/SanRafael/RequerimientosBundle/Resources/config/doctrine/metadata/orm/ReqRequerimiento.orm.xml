    </many-to-one>
    <many-to-one field="idEmpleadoSolicita" target-entity="ReqEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_empleado_solicita" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEquipoSolicitud" target-entity="ReqCtlEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_equipo_solicitud" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaTrabajo" target-entity="ReqCtlAreaTrabajo" fetch="LAZY">
      <join-columns>
        <join-column name="id_area_trabajo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
