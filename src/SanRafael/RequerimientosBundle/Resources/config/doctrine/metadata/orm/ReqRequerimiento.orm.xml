    <many-to-one field="idAreaTrabajo" target-entity="ReqCtlAreaTrabajo">
      <join-columns>
        <join-column name="id_area_trabajo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
