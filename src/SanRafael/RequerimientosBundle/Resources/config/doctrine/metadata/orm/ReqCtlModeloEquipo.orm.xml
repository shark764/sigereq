<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlModeloEquipo" table="req_ctl_modelo_equipo">
    <indexes>
      <index name="IDX_CE079FB1AE7383F0" columns="id_modelo_grupo"/>
      <index name="IDX_CE079FB15463B414" columns="id_marca_equipo"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_modelo_equipo" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_modelo_equipo_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="75" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Dell OptiPlex 9020</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="15" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">dlloptx9020</option>
      </options>
    </field>
    <field name="caracteristicas" type="text" column="caracteristicas" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idMarcaEquipo" target-entity="ReqCtlMarcaEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_marca_equipo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idModeloGrupo" target-entity="ReqCtlModeloEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_modelo_grupo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
