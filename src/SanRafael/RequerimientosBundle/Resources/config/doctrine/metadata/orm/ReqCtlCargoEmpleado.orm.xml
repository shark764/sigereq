<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlCargoEmpleado" table="req_ctl_cargo_empleado">
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_cargo_empleado" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="false"/>
    <field name="codigo" type="string" column="codigo" nullable="false"/>
  </entity>
</doctrine-mapping>
