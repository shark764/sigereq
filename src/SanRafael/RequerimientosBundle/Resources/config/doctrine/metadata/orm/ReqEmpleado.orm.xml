<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqEmpleado" table="req_empleado">
    <indexes>
      <index name="IDX_9ABCE344A7194A90" columns="id_sexo"/>
      <index name="IDX_9ABCE3447D36E8FB" columns="id_jefe_inmediato"/>
      <index name="IDX_9ABCE344D8A5832B" columns="id_user_reg"/>
      <index name="IDX_9ABCE344AC39DE56" columns="id_user_mod"/>
      <index name="IDX_9ABCE344F6BCBD1" columns="id_area_servicio_atencion"/>
      <index name="IDX_9ABCE3444F664059" columns="id_cargo_empleado"/>
      <index name="IDX_9ABCE344B13434FE" columns="id_tipo_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_empleado_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="apellido" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="habilitado" type="boolean" column="habilitado" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="correoElectronico" type="string" column="correo_electronico" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonoCasa" type="string" column="telefono_casa" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="telefonoCelular" type="string" column="telefono_celular" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="true"/>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true"/>
    <field name="horaNacimiento" type="time" column="hora_nacimiento" nullable="true"/>
    <field name="correoInstitucional" type="string" column="correo_institucional" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaContratacion" type="datetime" column="fecha_contratacion" nullable="true"/>
    <field name="fechaIniciaLabores" type="datetime" column="fecha_inicia_labores" nullable="true"/>
    <field name="fechaFinalizaLabores" type="datetime" column="fecha_finaliza_labores" nullable="true"/>
    <many-to-one field="idCargoEmpleado" target-entity="ReqCtlCargoEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_cargo_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoEmpleado" target-entity="ReqCtlTipoEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaServicioAtencion" target-entity="ReqAreaServicioAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_area_servicio_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idJefeInmediato" target-entity="ReqEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_jefe_inmediato" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSexo" target-entity="ReqCtlSexo" fetch="LAZY">
      <join-columns>
        <join-column name="id_sexo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
