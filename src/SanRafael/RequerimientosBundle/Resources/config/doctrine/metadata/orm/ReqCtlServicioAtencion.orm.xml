<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlServicioAtencion" table="req_ctl_servicio_atencion">
    <indexes>
      <index name="IDX_AC47A494C5DC305D" columns="id_atencion_padre"/>
      <index name="IDX_AC47A494A36B7986" columns="id_tipo_servicio"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_servicio_atencion" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_servicio_atencion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Unidad de Inform√°tica</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="6" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">INF</option>
      </options>
    </field>
    <many-to-one field="idTipoServicio" target-entity="ReqCtlTipoServicio" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_servicio" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAtencionPadre" target-entity="ReqCtlServicioAtencion" fetch="LAZY">
      <join-columns>
        <join-column name="id_atencion_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
