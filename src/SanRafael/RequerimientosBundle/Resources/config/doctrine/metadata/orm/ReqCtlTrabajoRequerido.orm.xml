<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlTrabajoRequerido" table="req_ctl_trabajo_requerido">
    <indexes>
      <index name="IDX_577A5776DDC7A485" columns="id_area_trabajo"/>
      <index name="IDX_577A5776E361769B" columns="id_trabajo_requerido_padre"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_trabajo_requerido" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="requerimiento" type="string" column="requerimiento" length="255" nullable="false"/>
    <field name="codigo" type="string" column="codigo" nullable="true"/>
    <many-to-one field="idTrabajoRequeridoPadre" target-entity="ReqCtlTrabajoRequerido">
      <join-columns>
        <join-column name="id_trabajo_requerido_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaTrabajo" target-entity="ReqCtlAreaTrabajo">
      <join-columns>
        <join-column name="id_area_trabajo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
