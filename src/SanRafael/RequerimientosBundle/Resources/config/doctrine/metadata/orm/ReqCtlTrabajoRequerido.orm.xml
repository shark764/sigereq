<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlTrabajoRequerido" table="req_ctl_trabajo_requerido">
    <indexes>
      <index name="IDX_577A5776E361769B" columns="id_trabajo_requerido_padre"/>
      <index name="IDX_577A5776DDC7A485" columns="id_area_trabajo"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_trabajo_requerido" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_trabajo_requerido_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="requerimiento" type="string" column="requerimiento" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Asignación de equipo de cómputo</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="6" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default">000000</option>
      </options>
    </field>
    <many-to-one field="idAreaTrabajo" target-entity="ReqCtlAreaTrabajo" fetch="LAZY">
      <join-columns>
        <join-column name="id_area_trabajo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTrabajoRequeridoPadre" target-entity="ReqCtlTrabajoRequerido" fetch="LAZY">
      <join-columns>
        <join-column name="id_trabajo_requerido_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
