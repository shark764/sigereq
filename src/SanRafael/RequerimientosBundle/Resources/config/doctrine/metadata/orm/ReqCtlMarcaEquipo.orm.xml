<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlMarcaEquipo" table="req_ctl_marca_equipo">
    <indexes>
      <index name="IDX_ED7C31B9CCF415DC" columns="id_marca_grupo"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_marca_equipo" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_marca_equipo_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">DELL</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="3" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">DLL</option>
      </options>
    </field>
    <field name="caracteristicas" type="text" column="caracteristicas" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idMarcaGrupo" target-entity="ReqCtlMarcaEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_marca_grupo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
