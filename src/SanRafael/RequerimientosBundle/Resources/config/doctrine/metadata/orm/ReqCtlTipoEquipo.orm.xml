<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlTipoEquipo" table="req_ctl_tipo_equipo">
    <indexes>
      <index name="IDX_3066CC92705370F4" columns="id_tipo_padre"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_tipo_equipo" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_tipo_equipo_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="75" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Computadora de escritorio</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="3" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">DKT</option>
      </options>
    </field>
    <field name="caracteristicas" type="text" column="caracteristicas" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idTipoPadre" target-entity="ReqCtlTipoEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
