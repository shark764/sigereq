<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlEstadoRequerimiento" table="req_ctl_estado_requerimiento">
    <indexes>
      <index name="IDX_3F450511FA5F996B" columns="id_estado_padre"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_estado_requerimiento" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_estado_requerimiento_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="75" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Requerimiento recibido</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="3" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">RRC</option>
      </options>
    </field>
    <many-to-one field="idEstadoPadre" target-entity="ReqCtlEstadoRequerimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_estado_padre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
