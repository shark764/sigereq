<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ReqCtlEquipo" table="req_ctl_equipo">
    <indexes>
      <index name="IDX_2DD770B369953885" columns="id_empleado_asignado"/>
      <index name="IDX_2DD770B366D95F61" columns="id_modelo_equipo"/>
      <index name="IDX_2DD770B3493768E4" columns="id_tipo_equipo"/>
      <index name="IDX_2DD770B3AC39DE56" columns="id_user_mod"/>
      <index name="IDX_2DD770B3D8A5832B" columns="id_user_reg"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_req_codigo_equipo" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="req_ctl_equipo_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">PC Dell Optiplex 9020 de prestaciones medias</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="10" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">000000</option>
      </options>
    </field>
    <field name="numeroInventario" type="string" column="numero_inventario" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="caracteristicas" type="text" column="caracteristicas" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaAdquisicion" type="datetime" column="fecha_adquisicion" nullable="true"/>
    <field name="fechaDespacho" type="datetime" column="fecha_despacho" nullable="true"/>
    <field name="idServicioAsignado" type="smallint" column="id_servicio_asignado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="serie" type="string" column="serie" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idUserReg" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoEquipo" target-entity="ReqCtlTipoEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo_equipo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idModeloEquipo" target-entity="ReqCtlModeloEquipo" fetch="LAZY">
      <join-columns>
        <join-column name="id_modelo_equipo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleadoAsignado" target-entity="ReqEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_empleado_asignado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
