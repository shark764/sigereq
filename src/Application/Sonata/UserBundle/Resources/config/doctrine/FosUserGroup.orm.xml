<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FosUserGroup" table="fos_user_group">
    <unique-constraints>
      <unique-constraint name="uniq_583d1f3e5e237e06" columns="name"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="fos_user_group_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="text" column="roles" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="user" target-entity="FosUserUser" mapped-by="group" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
